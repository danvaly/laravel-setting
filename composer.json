{
    "name": "danvaly/setting",
    "description": "Persistent settings package for Laravel.",
    "keywords": [
        "laravel",
        "persistent",
        "settings",
        "config"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Ghica",
            "email": "vali@danvaly.ro",
            "homepage": "https://www.danvaly.ro",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^9.19"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Danvaly\\Setting\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danvaly\\Setting\\Provider"
            ],
            "aliases": {
                "Setting": "Danvaly\\Setting\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
